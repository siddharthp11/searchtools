{
  "name": "rgx",
  "displayName": "rgx",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "1.103.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rgx.fn",
        "title": "rgx:fn"
      },
      {
        "command": "rgx.cla",
        "title": "rgx:cla"
      },
      {
        "command": "rgx.clacall",
        "title": "rgx:clacall"
      },
      {
        "command": "rgx.fncall",
        "title": "rgx:fncall"
      },
      {
        "command": "rgx.tbl",
        "title": "rgx:tbl"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
